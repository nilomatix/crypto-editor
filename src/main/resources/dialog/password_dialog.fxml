<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.*?>

<GridPane hgap="20" fx:controller="org.hsd.cryptoeditor.dialog.PasswordDialogController"
          maxHeight="-Infinity"
          maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="300.0"
          xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets top="25" right="10" bottom="0" left="10"/>
    </padding>
    <Label text="Password" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <PasswordField fx:id="passwordInput" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
</GridPane>
